node{
     stage('build'){
                     git 'https://github.com/sasidevops/myweb'
                    }
     stage('Maven Package'){
                            def mavenHome = tool name: 'maven-1', type: 'maven'
                            sh "${mavenHome}/bin/mvn" package
                   
                         }
      
      }                
                      
     
          
                    
